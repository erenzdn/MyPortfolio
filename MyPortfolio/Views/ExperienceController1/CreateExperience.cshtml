@model Experience
@{
    ViewData["Title"] = "CreateExperience";
    Layout = "~/Views/LayoutController1/Index.cshtml";
}

<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">Deneyimler</h4>
		<div class="row">
			<div class="col-md-12">
				<div class="card">
					<div class="card-header">
						<div class="card-title">Deneyim Listesi</div>
					</div>
					<div class="card-body">
						<div class="card-sub">
							This is the basic table view of the ready dashboard :
						</div>
						
						<form method="post">
							<label>Firma bilgisi</label>
							<input type="text" asp-for="Title" class="form-control" />
							<br />
							<label>Çalışılan pozisyon</label>
							<input type="text" asp-for="SubTitle" class="form-control" />
							<br />
							<label>Tarih Aralığı</label>
							<input type="text" asp-for="Date" class="form-control" />
							<br />
							<label>Açıklama</label>
							<textarea rows="10" class="form-control" asp-for="Description"></textarea>
							<br />
							<button class="btn btn-primary">Ekle</button>
						</form>


						
					</div>
				</div>


			</div>

		</div>
	</div>
</div>

